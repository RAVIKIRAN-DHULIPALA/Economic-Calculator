<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/clay2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ravi.diminishmethod.Main2Activity">
    <android.support.v7.widget.Toolbar
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:background="@color/colorPrimary"
        android:minHeight="50dp"
        app:titleTextColor="#FFFFFF"
        android:id="@+id/toolbar"
        app:popupTheme="@style/TextAppearance.AppCompat.Title"/>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="284dp"
        android:layout_height="174dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.106">

        <ImageButton
            android:id="@+id/dim"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_gravity="center"
            android:layout_marginLeft="30dp"
            android:background="#fff"
            android:src="@drawable/logo_th" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="-80dp"
            android:layout_marginTop="115dp"
            android:gravity="center"
            android:text="Interest calci"
            android:textSize="18dp" />

        <ImageButton
            android:id="@+id/ans"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_gravity="center"
            android:layout_marginLeft="70dp"
            android:background="#fff"
            android:src="@drawable/annal" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="-100dp"
            android:layout_marginTop="115dp"
            android:gravity="center"
            android:text="Annual savings"
            android:textSize="18dp" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="78dp">
        <TextView
            android:id="@+id/vt1"
            android:layout_width="match_parent"
            android:layout_height="155dp"
            android:layout_marginTop="300dp"
            android:fontFamily="cursive"
            android:textAlignment="center"
            android:textColor="#ff9800"
            android:textSize="25dp"
            tools:ignore="InvalidId" />
    </LinearLayout>

    <com.google.android.gms.ads.AdView xmlns:ads="http://schemas.android.com/apk/res-auto"
        android:id="@+id/adView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        ads:adSize="BANNER"
        ads:adUnitId="ca-app-pub-5094946833896975/1640643096"
        ads:layout_constraintBottom_toBottomOf="parent"
        ads:layout_constraintEnd_toEndOf="parent"
        ads:layout_constraintStart_toStartOf="parent"
        ads:layout_constraintTop_toTopOf="parent"
        ads:layout_constraintVertical_bias="0.984"></com.google.android.gms.ads.AdView>
</android.support.constraint.ConstraintLayout>
